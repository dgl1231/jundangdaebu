<div class="body__container">
    <div class="main-container">
        <div class="title_back">
            <div class="title">한도 문의</div>
        </div>
        <%if(login == 1 || login == 5){
        %>
        <div class="write"><a href="/write">글쓰기</a></div>
        <%}%>
        <%if(login == 0){
        %>
        <div class="write"><a href="/sign_up">글쓰기</a></div>
        <%}%>
        <table class="blueone">
            <tr>
                <th>No</th>
                <th>제목</th>
